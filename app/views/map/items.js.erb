var settings = {
  points : [ 
  <% for item in @items do %>
    <% item.coordinates.each do |place| %>
      {
        id     : "<%= item.id %>",
        lat    : <%= place.first %>,
        lon    : <%= place.last %>,
        image  : "<%= image_path 'map-popup.jpg' %>",
        type   : "<%= item.type %>",
        title  : "<%= item.title %>",
        creator: "<%= item.creator %>",
        score  : 0
      },
    <% end %>
  <% end %>
  ]
};
